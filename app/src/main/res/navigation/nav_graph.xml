<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.restaurantws.ui.categories.CategoriesFragment"
        android:label="Categorias"
        tools:layout="@layout/fragment_categories" >
        <action
            android:id="@+id/action_FirstFragment_to_productsFragment"
            app:destination="@id/productsFragment" />
    </fragment>
    <fragment
        android:id="@+id/productsFragment"
        android:name="com.example.restaurantws.ui.products.ProductsFragment"
        android:label="Products"
        tools:layout="@layout/fragment_products" >
        <argument
            android:name="category"
            app:argType="com.example.restaurantws.data.main.models.categorias.Category" />
    </fragment>
    <fragment
        android:id="@+id/pedidosFragment"
        android:name="com.example.restaurantws.ui.pedidos.PedidosFragment"
        android:label="PedidosFragment" /><action android:id="@+id/action_global_pedidosFragment" app:destination="@id/pedidosFragment"/>
</navigation>